PY_PYTHON=python${BUILD_PYTHON_VERSION}

sudo apt-get update && sudo apt-get install -y unixodbc unixodbc-dev

wget -O pypyodbc.tar.gz https://github.com/jiangwen365/pypyodbc/archive/1.3.5.tar.gz
tar -xf pypyodbc.tar.gz

cd pypyodbc-1.3.5
$PY_PYTHON setup.py install --root $BUILD_DIR/python-install
